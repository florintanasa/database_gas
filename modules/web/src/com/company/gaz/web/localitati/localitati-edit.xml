<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.gaz.web.localitati.LocalitatiEdit"
        datasource="localitatiDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.gaz.web.localitati">
    <dsContext>
        <datasource id="localitatiDs"
                    class="com.company.gaz.entity.Localitati"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="localitatiDs">
            <column width="250px">
                <field caption="msg://numeLocalitate"
                       property="numeLocalitate"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
